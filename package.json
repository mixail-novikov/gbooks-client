{
  "name": "gbooks-client-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/query-string": "^6.1.0",
    "apollo": "^1.7.0",
    "apollo-boost": "^0.1.13",
    "graphql": "^0.13.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "recompose": "^0.28.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "download_schema": "apollo schema:download --endpoint http://localhost:4000",
    "generate_types": "apollo codegen:generate --target=typescript --schema=schema.json",
    "make_schema": "npm run download_schema && npm run generate_types"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/recompose": "^0.26.4",
    "typescript": "^3.0.1"
  }
}
